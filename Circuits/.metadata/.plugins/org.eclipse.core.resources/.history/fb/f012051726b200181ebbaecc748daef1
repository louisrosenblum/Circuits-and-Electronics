;Main.s by Anthony Louis Rosenblum
;9/6/18
;Simple program to toggle LED1 on the MC9S08QG8

		INCLUDE 'derivative.inc'
		XDEF _Startup, main
		XREF __SEG_END_SSTACK
		
main:
_Startup:
		BSET 6, PTBDD
		BSET 7, PTBDD
mainLoop:
		BSET 6, PTBD
		
		EOR PTBD6
		AND 0
		
		EOR PTBD6
		AND 1
		
		BSET 7, PTBD
		
		EOR PTBD7
		AND 0
		
		EOR PTBD7
		AND 1
		
		
		BRA mainLoop
